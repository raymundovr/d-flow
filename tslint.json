{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "ordered-imports": false,
        "interface-name": false,
        "object-literal-sort-keys": false
    },
    "linterOptions": {
        "exclude": [
            "__tests__"
        ]
    }
}
